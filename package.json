{
  "name": "qbittorrent-web-ui",
  "version": "0.0.1",
  "main": "app/main.js",
  "repository": "https://github.com/drsepanta/qbittorrent-web-ui.git",
  "author": "Dr. Sepanta <dr_sepanta@yahoo.com>",
  "license": "MIT",
  "scripts": {
    "start": "run-p \"serve-dev\" \"typecheck -- -w\"",
    "prestart": "npm run intl:compile",
    "serve-dev": "webpack serve",
    "typecheck": "tsc --noEmit",
    "build": "NODE_ENV=production webpack --env=production",
    "prebuild": "rimraf build/* && npm run intl:compile",
    "intl:extract": "formatjs extract 'src/**/*.ts*' --out-file src/lang/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "intl:compile": "formatjs compile-folder --ast src/lang public/lang",
    "intl": "run-s \"intl:*\""
  },
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "clsx": "^1.1.1",
    "color": "^3.1.3",
    "dayjs": "^1.9.8",
    "dom-helpers": "^5.2.0",
    "immer": "^8.0.0",
    "lodash": "^4.17.20",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-intl": "^5.10.10",
    "react-query": "^3.5.6",
    "react-router-dom": "^5.2.0",
    "react-virtualized": "^9.22.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-numeric-separator": "^7.12.7",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/runtime": "^7.12.5",
    "@formatjs/cli": "^2.13.18",
    "@types/color": "^3.0.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-virtualized": "^9.21.10",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-react-intl": "^8.2.24",
    "babel-plugin-transform-imports": "^2.0.0",
    "copy-webpack-plugin": "^5.0.0",
    "dotenv-flow": "^3.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3",
    "webpack": "^4.0.0",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^4.0.0",
    "webpack-dev-server": "^3.0.0",
    "webpack-merge": "^5.7.3"
  }
}
